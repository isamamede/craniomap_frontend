{
  "expo": {
    "name": "face-mesures",
    "slug": "face-mesures",
    "version": "1.0.0",
    "assetBundlePatterns": ["**/*"],
    "icon": "./src/assets/icon.png",
    "splash": { "image": "./src/assets/splash.png" },
    "android": {
      "package": "com.medtown.facemesures"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Please allow acess to your photos!",
          "cameraPermission": "Please allow acess to your camera!"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
